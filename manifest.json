{
    "manifest_version": 2,
    "name": "LightShot Downloader",
    "version": "1.0",
    "description": "Extracts the URL from prnt.sc domains",
    "author": "Yaber Hasan",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "permissions": ["storage"],
    "browser_action": {
      "default_popup": "popup.html"
    }
  }
  